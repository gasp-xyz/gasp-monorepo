{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"style": {
				"noNonNullAssertion": "off",
				"noUselessElse": "off"
			},
			"correctness": {
				"noConstantCondition": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	}
}
