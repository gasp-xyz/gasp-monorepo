{
  "name": "stash",
  "version": "1.0.0",
  "author": "Gasp",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "FORCE_COLOR=true yarn lint:eslint & FORCE_COLOR=true yarn lint:tsc",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:staged": "yarn pretty-quick && lint-staged",
    "pretify": "prettier --write .",
    "build-ts": "tsc",
    "build-ts2": "tsc",
    "build": "rimraf dist/ && yarn run build-ts && yarn run swagger",
    "debug": "yarn run build && yarn run watch-debug",
    "serve-debug": "nodemon --inspect -r source-map-support/register dist/server.js",
    "start": "NODE_OPTIONS=--enable-source-maps node dist/server.js",
    "start-local": "yarn run build && yarn run start",
    "gcp-build": "rimraf dist/ && yarn run build-ts",
    "test": "yarn vitest --run ",
    "test-ci": "yarn vitest --run --coverage --passWithNoTests",
    "test:api": "yarn vitest --run test/api",
    "test:unit": "yarn vitest --run test/*.unit.test.ts --passWithNoTests",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "yarn vitest",
    "watch-ts": "tsc -w",
    "swagger": "node swagger.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.3.0",
    "@polkadot/api": "^11.1.1",
    "@polkadot/api-base": "10.12.2",
    "@polkadot/rpc-core": "10.12.2",
    "@polkadot/rpc-provider": "10.12.2",
    "@polkadot/types": "^11.1.1",
    "@polkadot/types-codec": "11.3.1",
    "@polkadot/util": "^12.3.2",
    "@polkadot/util-crypto": "^12.3.2",
    "async": "^3.2.4",
    "async-redis": "^2.0.0",
    "axios": "^1.7.2",
    "bignumber.js": "^9.1.2",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "core-js": "^3.29.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "decimal.js": "^10.4.2",
    "decimal.js-light": "^2.5.1",
    "dotenv": "16.0.3",
    "errorhandler": "1.5.1",
    "express": "^4.19.2",
    "express-validator": "6.14.2",
    "gasp-sdk": "0.0.2-eth-rollup-develop.223",
    "gasp-types": "0.0.2-eth-rollup-develop.223",
    "ioredis": "^5.3.0",
    "joi": "^17.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "redis": "^4.7.0",
    "redis-om": "^0.4.6",
    "redlock": "^5.0.0-beta.2",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^5.0.0",
    "viem": "^2.23.9",
    "winston": "3.8.2",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/async": "3.2.15",
    "@types/big.js": "^6.1.6",
    "@types/chai": "^4",
    "@types/concurrently": "6.4.0",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "8.4.6",
    "@types/express": "4.17.14",
    "@types/ioredis": "^4.28.8",
    "@types/lodash": "^4",
    "@types/node": "18.8.2",
    "@types/request": "2.48.8",
    "@types/request-promise": "4.1.48",
    "@types/shelljs": "0.8.11",
    "@types/source-map-support": "^0",
    "@types/supertest": "2.0.12",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/coverage-c8": "^0.29.7",
    "concurrently": "7.4.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nodemon": "^3.1.4",
    "prettier": "^2.8.6",
    "pretty-quick": "^3.1.3",
    "sass": "1.55.0",
    "semantic-release": "^24.2.3",
    "semantic-release-monorepo": "^8.0.2",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "swagger-autogen": "latest",
    "testcontainers": "^9.11.0",
    "ts-node": "^10.9.2",
    "typescript": "5.4.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "resolutions": {
    "@polkadot/types-codec": "11.3.1",
    "@polkadot/types": "^11.1.1",
    "@polkadot/rpc-core": "11.3.1",
    "@polkadot/rpc-augment": "11.3.1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "yarn eslint --cache --fix"
  },
  "dependenciesMeta": {
    "vitest@0.29.7": {
      "unplugged": true
    }
  },
  "packageManager": "yarn@4.7.0"
}
